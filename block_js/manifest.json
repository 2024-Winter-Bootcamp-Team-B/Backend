{
    "manifest_version": 3,
    "name": "Site Blocker",
    "version": "1.0",
    "permissions": [
        "declarativeNetRequest", 
        "declarativeNetRequestWithHostAccess",
        "storage"
    ],
    "host_permissions": [
        "*://*/*" 
    ],
    "background": {
        "service_worker": "background.js"
    },
    "description": "사용자별 차단 사이트를 관리하는 Chrome 확장 프로그램"
}